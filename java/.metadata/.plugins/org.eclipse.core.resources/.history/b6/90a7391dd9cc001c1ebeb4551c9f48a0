package students;

public class Student {
	public String id;
	public String name;
	
	public Student() {}
	public Student(String id, String name) {
		this.id = id;
		this.name = name;
	}
	
	@Override
	public String toString() {
		System.out.printf("[Student] id=%s, name =%s%n", this.id, this.name);
	}
}
