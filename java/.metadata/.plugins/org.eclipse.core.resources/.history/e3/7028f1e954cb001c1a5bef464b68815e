package calc;

public abstract class Calc2 implements Calcable {
	
	@Override
	public int add(int n1, int n2) {
		int x = n1 + n2;
		return x;
	}

	@Override
	public int sub(int n1, int n2) {
		int x = n1 - n2;
		return x;
	}

	@Override
	public int mul(int n1, int n2) {
		int x = n1 * n2;
		return x;
	}

	@Override
	public int div(int n1, int n2) {
		int x = n1 / n2;
		return x;
	}
	
	public abstract int tot();
}
